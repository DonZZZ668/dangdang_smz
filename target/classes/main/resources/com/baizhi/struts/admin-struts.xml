<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="admin" extends="struts-default" namespace="/admin">
    	<action name="login" class="com.baizhi.action.AdminAction" method="login">
    		<result name="loginSuccess">/back/main/main.jsp</result>
    		<result name="error">/back/login.jsp</result>
    	</action>
    	<action name="getCode" class="com.baizhi.action.AdminAction" method="getCode"></action>
    	<action name="signOut" class="com.baizhi.action.AdminAction" method="signOut">
    		<result name="outSuccess">/back/login.jsp</result>
    	</action>
    	
    	
    </package>
</struts>
