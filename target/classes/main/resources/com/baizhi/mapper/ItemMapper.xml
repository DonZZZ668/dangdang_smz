<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.ItemDao">
	<insert id="addOneItem" parameterType="Item">
		insert into dd_item(id,order_id,book_id,count,create_date,name,cover,price,dprice) values(#{id},#{orderId},#{bookId},#{count},#{createDate},#{name},#{cover},#{price},#{dprice})
	</insert>
</mapper>
